{
  "AWSEBDockerrunVersion": "2",
  "Logging": "/opt/nifi/logs",
  "Volumes": [
    {
      "name": "efs",
      "HostDirectory": "/efs",
      "ContainerDirectory": "/opt/nifi/certs"
    }
  ],
  "containerDefinitions": [
    {
      "name": "nifi",
      "image": "apache/nifi:1.6.0",
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        },
        {
          "hostPort": 80,
          "containerPort": 9090
        }
      ]
    }
  ]
}
